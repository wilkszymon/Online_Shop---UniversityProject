        <Button x:Name="ButtonBack" Content="Wróć" Background="#FFE01616" Foreground="#FFFBF8F8" BorderBrush="{x:Null}" HorizontalAlignment="Left" Height="50" Margin="216,562,0,0" VerticalAlignment="Top" Width="200" Click="ButtonBack_Click" RenderTransformOrigin="0.503,-1.475" FontSize="15" FontWeight="bold"/>

