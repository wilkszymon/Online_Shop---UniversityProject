            <Label x:Name="City" Content="Miejscowość" HorizontalAlignment="Left" Height="31" Margin="131.427,173,0,0" VerticalAlignment="Top" Width="189" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="11" FontStyle="Italic" Grid.Column="1"/>
